{
  "entryPoints": ["src/utils/*.ts"],
  "out": "docs/api",
  "readme": "none",
  "plugin": ["typedoc-plugin-markdown"],
  "exclude": ["src/index.ts", "**/components/**", "**/composables/**", "**/*.vue"],
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "disableSources": true,
  "hideBreadcrumbs": true,
  "githubPages": false,

  "markdown": {
    "theme": "default",
    "showInherited": false,
    "showStatic": true,
    "showPrivate": false,
    "showProtected": false,
    "showDeprecated": false,
    "showEvents": false,
    "showConstructors": false,
    "showIndexSignatures": false,
    "showOverloads": false,
    "showExamples": true,
    "showTypeDeclarations": true,
    "showModuleFilenames": false,
    "showModuleMembers": true,
    "showEnumValues": true,
    "showGettersSetters": true,
    "showMethodReturns": true,
    "showPropertyTypes": true,
    "showParameterTypes": true,
    "showSignature": true,
    "showSourceCode": false
  }
}
